  <%= stylesheet_link_tag    '/鸟窝html/stylesheets/ui_common.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    '/鸟窝html/stylesheets/register.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    '/鸟窝html/stylesheets/popup.css', media: 'all', 'data-turbolinks-track' => true %>
